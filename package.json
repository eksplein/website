{
  "name": "eksplein-client",
  "description": "eksplein-client",
  "version": "0.0.2",
  "author": "Tom Bazarnik <tommywalkie@gmail.com>",
  "license": "GPL-3.0",
  "scripts": {
    "dev": "npm run excalidraw && sapper dev --port 4000",
    "build": "npm run excalidraw && sapper build",
    "export": "npm run license-compliance && npm run excalidraw && sapper export",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "excalidraw": "excalidraw-cli excalidraw ./static/excalidraw",
    "license-compliance": "license-compliance --production --allow \"MIT;WTFPL;ISC;Apache-2.0;0BSD;BSD-3-Clause;BSD-2-Clause;GPL-2.0;GPL-2.0+;GPL-3.0;LGPL-2.0;LGPL-3.0;LGPL-3.0+;MPL-2.0\"",
    "docma": "docma && docma serve"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "date-fns": "^1.30.1",
    "gray-matter": "^4.0.2",
    "marked": "^0.6.2",
    "polka": "^0.5.0",
    "prism-svelte": "^0.4.6",
    "prismjs": "^1.20.0",
    "reading-time": "^1.2.0",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.10.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/runtime": "^7.10.3",
    "@rollup/plugin-babel": "^5.0.3",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "@rollup/plugin-replace": "^2.3.3",
    "@tommywalkie/excalidraw-cli": "^0.5.0",
    "cypress": "^4.7.0",
    "cypress-jest-adapter": "^0.1.1",
    "docma": "^3.2.2",
    "fs-extra": "^9.0.1",
    "license-compliance": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.13.1",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-svg-import": "^1.5.1",
    "rollup-plugin-terser": "^4.0.4",
    "sapper": "^0.27.13",
    "svelte": "^3.0.0"
  }
}
